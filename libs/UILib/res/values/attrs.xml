<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="quick">
        <attr name="bottom_tab_bar_background" format="reference|color"/>
        <attr name="bottom_tab_bar_top_bottom_padding" format="dimension" />
        <attr name="bottom_tab_item_background" format="reference|color"/>
        <attr name="bottom_tab_item_textColor" format="reference|color"/>
        <attr name="bottom_tab_item_textSize" format="dimension"/>
        <attr name="bottom_tab_item_has_new_textColor" format="reference|color"/>
        <attr name="bottom_tab_item_has_new_textSize" format="dimension"/>
        <attr name="bottom_tab_item_minimal_dot_radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="isRoundBgDisplayable" format="boolean"/>
        <attr name="roundBgColor" format="color" />
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textcolor" format="color" />
        <attr name="textsize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>
